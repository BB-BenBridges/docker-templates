<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Privileged>true</Privileged>
  <Beta>False</Beta>
  <Category>Backup: Tools:</Category>
  <Name>steamcache-dns</Name>
  <Description>This docker container provides DNS entries for *.cs.steampowered.com to be used in conjunction with a steamcache server.</Description>
  <Overview/>
  <Support/>
  <Registry>https://hub.docker.com/r/steamcache/steamcache-dns/</Registry>
  <GitHub>https://steamcache.github.io/</GitHub>
  <Repository>steamcache/steamcache-dns:latest</Repository>
  <Network>host</Network>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>53</HostPort>
        <ContainerPort>53</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>53</HostPort>
        <ContainerPort>53</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Value>111.111.111.111</Value>
      <Name>steamcache-ip</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Data/>
  <WebUI/>
  <Icon>http://www.thekelleys.org.uk/dnsmasq/images/icon.png</Icon>
  <Config Name="steamcache-ip" Target="steamcache-ip" Default="111.111.111.111" Mode="" Description="IP Address of your unRAID server" Type="Variable" Display="always" Required="true" Mask="false">111.111.111.111</Config>
  <Config Name="DNS UDP Port" Target="53" Default="53" Mode="udp" Description="Container Port: 53" Type="Port" Display="advanced" Required="true" Mask="false">53</Config>
  <Config Name="DNS TCP Port" Target="53" Default="53" Mode="tcp" Description="Container Port: 53" Type="Port" Display="advanced" Required="true" Mask="false">53</Config>
</Container>
